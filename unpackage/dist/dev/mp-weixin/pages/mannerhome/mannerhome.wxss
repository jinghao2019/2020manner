
.all{
	background-color: #f5f5f5;
	position: relative;
}
.list{
	height: 100rpx;
	padding: 0 60rpx;
	margin: auto;
	background-color: #ffffff;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.title{
	margin-bottom: 270rpx;
}
.card{
	position: absolute;
	top: 140rpx;
	left: 40rpx;
	background-color: #ffffff;
	box-shadow: 0 1rpx 10rpx 5rpx #cccccc;
	border-radius: 10rpx;
	width: 80%;
	padding: 40rpx;
}
.card1{
	border-bottom: 1rpx solid #c8c8c8;
	padding-bottom: 20rpx;
}
.card2>view>view:first-child{
	margin-top: 20rpx;
	color:#610000;
	font-weight: 600;
}
.card2>view>view:last-child{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	font-weight: 600;
}
.order{
	margin-bottom: 20rpx;
}
.middle{
}
.manner{
	margin-top: 20rpx;
}

